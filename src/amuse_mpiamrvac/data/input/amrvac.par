 &testlist
 /

 &filelist
        filenamelog='data/mpiamrvac/output'
        filenameout='data/mpiamrvac/output'
        primnames= 'rho v1 v2 v3 p'
/

 &savelist
        itsave(1,1)=0
        itsave(1,2)=0
        dtsave(2)=0.05d0
 /

 &stoplist
        tmax=1.0d0
        dtmin=1.D-6
 /

 &methodlist
        wnames=       'rho m1 m2 m3 e'
        fileheadout=  'amuse33'
        fileheadout=  'rt_hd33'
        typeadvance=  'twostep'
        typefull1=    13*'tvdmu'
        typepred1=    13*'tvdmu'
        typelimiter1= 13*'woodward'
        typelimited='predictor'
        dimsplit=.false.
        sourceunsplit=.true.
        sourcesplit=.false.
 /

 &boundlist
        dixB=   2
        typeB=   5*'periodic',5*'periodic',5*'periodic',5*'periodic',5*'periodic',5*'periodic'
        typeghostfill= 'linear'
/

 &amrlist
        errorestimate=3
        mxnest=3
        tol=13*0.5d0
        nxlone1=24
        nxlone2=24
        nxlone3=24
        xprobmin1=-0.5d0
        xprobmax1=0.5d0
        xprobmin2=-0.5d0
        xprobmax2=0.5d0
        xprobmin3=-0.5d0
        xprobmax3=0.5d0
        iprob=6
 /

 &paramlist
        typecourant='maxsum'
        courantpar=0.8d0
        slowsteps=10
 /
